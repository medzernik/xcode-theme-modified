{
  "name": "xcode-12-theme-medzernik",
  "displayName": "Xcode Theme Custom",
  "description": "Xcode Theme (medzernik version), ported to VS Code",
  "icon": "images/icon.png",
  "version": "4.2.1",
  "scripts": {
    "build": "vsce package"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "publisher": "medzernik",
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medzernik/xcode-theme-modified"
  },
  "contributes": {
    "themes": [
      {
        "label": "Xcode Classic Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Classic Dark.json"
      },
      {
        "label": "Xcode Classic Light",
        "uiTheme": "vs",
        "path": "./themes/Xcode Classic Light.json"
      },
      {
        "label": "Xcode Default Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Default Dark.json"
      },
      {
        "label": "Xcode Default Dark Customized Version",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Default Dark Customized Version.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.9.0"
  }
}
